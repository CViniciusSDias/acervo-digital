{% extends "::base.html.twig" %}

{% block body %}
    <h2>{{ titulo }}</h2>

    <ul class="list-group">
        {% for trabalho in trabalhos %}
            <li class="list-group-item">
                <a href="{{ path('trabalho', {'trabalho': trabalho.id}) }}">{{ trabalho.titulo }} - {{ trabalho.anoPublicacao }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
